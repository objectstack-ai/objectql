    "test": "jest"
  },
  "peerDependencies": {
    "@objectql/core": "workspace:*",
    "@objectql/types": "workspace:*",
    "@objectql/driver-sql": "workspace:*",
    "@objectql/platform-node": "workspace:*",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@objectql/core": "workspace:*",
    "@objectql/types": "workspace:*",
    "@objectql/cli": "workspace:*",
    "@objectql/driver-sql": "workspace:*",
    "@objectql/platform-node": "workspace:*",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0"
  }
}
